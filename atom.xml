<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Devillers - Freelance Software Architect</title>
 <link href="http://www.devillers.nl/atom.xml" rel="self"/>
 <link href="http://www.devillers.nl/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.devillers.nl</id>
 <author>
   <name>Martin Devillers</name>
   <email>martin@devillers.nl</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.devillers.nl{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.devillers.nl{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
